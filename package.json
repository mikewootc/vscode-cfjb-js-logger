{
  "name": "cfjb-js-logger",
  "displayName": "vscode-cfjb-js-logger",
  "description": "Vscode configurable js logger",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "aliases": ["JavaScript", "javascript", "js"],
        "extensions": [".js"]
      },
      {
        "id": "typescript",
        "aliases": ["TypeScript", "typescript", "ts"],
        "extensions": [".ts"]
      }
    ],
    "commands": [
      {
        "command": "extension.log",
        "title": "Configurable js logger' log"
      }
    ],
    "keybindings": [
      {
        "command": "extension.log",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "cfjb-js-logger configuration",
      "properties": {
        "loggerFunction": {
          "type": "string",
          "default": "logger.debug",
          "description": "Logger's log function string."
        },
        "insertFunctionNameEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable inserting function name."
        },
        "functionNameSuffix": {
          "type": "string",
          "default": "_",
          "description": "Function name suffix which will be added after function name string."
        },
        "useSemiColon": {
          "type": "boolean",
          "default": "true",
          "description": "Use semi colon after function call."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
